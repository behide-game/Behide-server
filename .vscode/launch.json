  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build: solution",
      "program": "${workspaceFolder}/src/bin/Debug/net7.0/BehideServer.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "Tests",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build: solution",
      "program": "${workspaceFolder}/tests/bin/Debug/net7.0/BehideServer.Tests.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopAtEntry": false
    }
  ]
}